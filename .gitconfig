  email = guilherme.costa@timeneon.com.br
[core]
  editor = vim
[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  count = "!f() { compareBranch=${1-dev}; git rev-list --count HEAD ^$compareBranch; }; f"
[color]
  ui = true
  branch = auto
  status = auto
